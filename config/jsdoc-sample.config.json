{
    "docdash": {
        "search": true,
        "typedefs": true,
        "collapse": true,
        "sort": false,
        "sectionOrder": [
            "Tutorials",
            "Classes",
            "Namespaces",
            "Modules",
            "Externals",
            "Events",
            "Mixins",
            "Interfaces",
            "Global"
        ],
        "scripts": [ "docdash.min.js" ]
    },
    "opts": {
        "destination": "./jsdoc-sample/",
        "private": true,
        "recurse": true,
        "readme": "sample/README.md",
        "template": "node_modules/docdash"
    },
    "plugins": [ 
        "plugins/markdown",
        "."
    ],
    "submodule": {
        "roots": {
            "A": "sample/includeA/moduleA",
            "B": "sample/includeB/moduleB",
            "A/spec": "sample/includeA/spec"
        }
    },
    "recurseDepth": 10,
    "source": {
        "include": ["sample/includeA", "sample/includeB"],
        "includePattern": ".+\\.js(doc|x)?$"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "staticFiles": {
                "include": [
                    "dist/docdash.min.js",
                    "dist/docdash.min.js.map"
                ]
            }
        }
    }
}